/*
*  Customization overrides for integration in drupal theme.
*/

h2 {
  line-height: inherit;
}

span.ext {
        display:block;
}

a:hover,
a:focus {
  text-decoration: none;
  color: #1C6E84;
}

#sengines{	
	width:131px;	
	background-image: url(/sites/all/themes/omega/omega/images/select-arrow.gif);
	background-repeat: no-repeat;
	background-position: right center;
}

#for{
	width:157px;
}

#block-block-44 {
	margin-top: -90px;
}

.breadcrumb {
	border-bottom: thin solid #660033;
	font-size: 90%;
	margin-top: -12px;
	margin-bottom: 18px;
	padding-top: 0px;
	padding-right: 8px;
	padding-bottom: 4px;
	padding-left: 8px;
	background-color: #fff;
	-webkit-border-radius: 0px;
	-moz-border-radius: 0px;
	border-radius: 0px;
}

label {
	margin:0px;
	font:inherit;
	font-weight: bold;
	display: block;
}

.d1-anchor-style {
	color:#660033;
}

.container {
    width:700px;
}

#Content {
	padding-top:0px;
	margin-top:-30px;
}

.btn-default {
	color:black;
}

input[type="text"], select {
	-webkit-border-radius:0;
	border-radius: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-moz-background-clip: padding;
	-webkit-background-clip: padding;
	background-clip: padding-box;
	background-color: #fff;
	border: 1px solid;
	border-color: #848484 #c1c1c1 #e1e1e1;
	color: #000;
	outline: 0;
	padding: 2px 3px;
	font-size: 13px;
	font-family: Arial, sans-serif;
	height: 1.8em;
	margin: 0;
	vertical-align: middle;
	line-height: 1;
}

html, body {
	background-color:#1C6E84;		
	font: 13px/1.5 Helvetica, Arial,sans-serif;
}

#map-canvas {
    width: 100%;
    height: 500px;
    position: relative;
    margin: 5px 0px 0px 0px;
}

#map-legend {
    position: absolute;
    top: 135px;
    right: 1px;
    color: #555555;
    background-color: #F2F2F2;
    opacity: .9;
    width: 18em;
    border-radius: 6px;
    margin: 10px 10px 10px 10px;
    z-index: 10000;    
}

#map-legend td {
    padding: 0px;
    margin: 0px;
}

#map-legend img {
    padding: 5px;
    margin: 0px;
}

.d1-anchor-style:hover {
	color:#066;
}

table tbody tr td a:hover .node-stats-icon {
	color:#1C6E84;
}

